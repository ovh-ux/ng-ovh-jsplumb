{
  "name": "@ovh-ux/ng-ovh-jsplumb",
  "version": "4.0.0",
  "description": "Allow to draw links between elements using [jsplumb toolkit](http://www.jsplumb.org/).",
  "keywords": [
    "angularjs",
    "jsplumb"
  ],
  "repository": "ovh-ux/ng-ovh-jsplumb",
  "license": "BSD-3-Clause",
  "author": "OVH SAS",
  "files": [
    "dist"
  ],
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "browser": "./dist/umd/ng-ovh-jsplumb.js",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c --watch",
    "lint": "run-p lint:js lint:md",
    "lint:js": "eslint --quiet --fix --format=pretty src",
    "lint:md": "eslint --quiet --fix --format=pretty --ext .md .",
    "prepare": "rollup -c",
    "test": "yarn run lint",
    "version": "npx conventional-changelog-cli -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "postversion": "git push && git push --tags"
  },
  "dependencies": {
    "lodash": "^4.17.11",
    "urijs": "^1.19.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.1.2",
    "@commitlint/config-angular": "^7.1.2",
    "@ovh-ux/component-rollup-config": "^5.0.0-beta.9",
    "eslint": "^5.15.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-formatter-pretty": "^2.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-markdown": "^1.0.0",
    "husky": "^1.1.0",
    "lint-staged": "^8.1.5",
    "npm-run-all": "^4.1.5"
  },
  "peerDependencies": {
    "angular": "~1.6",
    "jquery": "~2.1",
    "jsplumb": "^2.9.0"
  }
}
